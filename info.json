{
  "id": "plugin.com.TzuLao.deepseek",
  "plugin_type": "translate",
  "icon": "deepseek.svg",
  "display": "deepseek",
  "homepage": "https://github.com/tzulao55/pot-app-translate-plugin-deepseek",
  "needs": [
    {
      "key": "apiEndpoint",
      "display": "API地址(可选)",
      "type": "input",
      "default": "https://api.deepseek.com/chat/completions",
      "attrs": {
        "placeholder": "https://api.deepseek.com/chat/completions"
      }
    },
    {
      "key": "model",
      "display": "模型选择(可选)",
      "type": "input",
      "default": "deepseek-chat",
      "attrs": {
        "placeholder": "deepseek-chat"
      }
    },
    {
      "key": "apiKey",
      "display": "API密钥(必填)",
      "type": "input"
    },
    {
      "key": "temperature",
      "display": "temperature(可选)",
      "type": "input",
      "min": 0,
      "max": 2,
      "step": 0.1,
      "default": 0.1,
      "attrs": {
        "placeholder": "0.1"
      }
    },
    {
      "key": "top_p",
      "display": "top_p(可选)",
      "type": "input",
      "min": 0,
      "max": 1,
      "step": 0.01,
      "default": 0.99,
      "attrs": {
        "placeholder": "0.99"
      }
    },
    {
      "key": "max_tokens",
      "display": "max_tokens(可选)",
      "type": "input",
      "min": 100,
      "max": 4000,
      "default": 2000,
      "attrs": {
        "placeholder": "2000"
      }
    }
  ],
  "language": {
    "auto": "auto",
    "zh_cn": "zh",
    "zh_tw": "zh_HANT",
    "en": "en",
    "ja": "ja",
    "ko": "ko",
    "fr": "fr",
    "es": "es",
    "ru": "ru",
    "de": "de",
    "it": "it",
    "tr": "tr",
    "pt_pt": "pt",
    "pt_br": "pt",
    "vi": "vi",
    "id": "id",
    "th": "th",
    "ms": "ms",
    "ar": "ar",
    "hi": "hi",
    "mn_cy": "mn",
    "mn_mo": "mn",
    "km": "km",
    "nb_no": "no",
    "nn_no": "no",
    "fa": "fa"
  }
}
